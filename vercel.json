      "dest": "/api/admin/mailing/sync-customers.js"
    },
    {
      "src": "/api/admin/mailing/incremental-sync",
      "dest": "/api/admin/mailing/incremental-sync.js"
    },
    {
      "src": "/api/admin/mailing/process-batch-jobs",
      "dest": "/api/admin/mailing/process-batch-jobs.js"
    },
    {
      "src": "/api/admin/mailing/webhook",
      "dest": "/api/admin/mailing/webhook.js"
