{
  "version": 2,
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/admin/mailing/process-batches",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/admin/mailing/process-batch-jobs",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/api/admin/mailing/incremental-sync",
      "schedule": "0 */6 * * *"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/package/submit",
      "dest": "/api/package/submit.js"
    },
    {
      "src": "/api/package/send-email",
      "dest": "/api/package/send-email.js"
    },
    {
      "src": "/api/package/create-payment",
      "dest": "/api/package/create-payment.js"
    },
    {
      "src": "/api/package/payment-webhook",
      "dest": "/api/package/payment-webhook.js"
    },
    {
      "src": "/api/package/generate-agreement-pdf",
      "dest": "/api/package/generate-agreement-pdf.js"
    },
          {
            "src": "/api/admin/auth",
            "dest": "/api/admin/auth.js"
          },
          {
            "src": "/api/unsubscribe",
            "dest": "/api/unsubscribe.js"
          },
    {
      "src": "/api/admin/overview",
      "dest": "/api/admin/overview.js"
    },
    {
      "src": "/api/admin/finance",
      "dest": "/api/admin/finance.js"
    },
    {
      "src": "/api/admin/customers",
      "dest": "/api/admin/customers.js"
    },
    {
      "src": "/api/admin/search-customers",
      "dest": "/api/admin/search-customers.js"
    },
    {
      "src": "/api/admin/mailing/consumers",
      "dest": "/api/admin/mailing/consumers.js"
    },
    {
      "src": "/api/admin/mailing/templates",
      "dest": "/api/admin/mailing/templates.js"
    },
    {
      "src": "/api/admin/mailing/workflows",
      "dest": "/api/admin/mailing/workflows.js"
    },
    {
      "src": "/api/admin/mailing/lists",
      "dest": "/api/admin/mailing/lists.js"
    },
    {
