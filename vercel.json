{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/package/submit",
      "dest": "/api/package/submit.js"
    },
    {
      "src": "/api/package/send-email",
      "dest": "/api/package/send-email.js"
    },
    {
      "src": "/api/package/create-payment",
      "dest": "/api/package/create-payment.js"
    },
    {
      "src": "/api/package/payment-webhook",
      "dest": "/api/package/payment-webhook.js"
    },
    {
      "src": "/api/package/generate-agreement-pdf",
      "dest": "/api/package/generate-agreement-pdf.js"
    },
    {
      "src": "/api/admin/overview",
      "dest": "/api/admin/overview.js"
    },
    {
      "src": "/api/admin/finance",
      "dest": "/api/admin/finance.js"
    },
    {
      "src": "/api/admin/customers",
      "dest": "/api/admin/customers.js"
    },
    {
      "src": "/api/admin/search-customers",
      "dest": "/api/admin/search-customers.js"
    },
    {
      "src": "/api/seller-admin/orders",
      "dest": "/api/seller-admin/orders.js"
    },
    {
      "src": "/api/seller-admin/order",
      "dest": "/api/seller-admin/order.js"
    },
    {
      "src": "/api/seller-admin/balance",
      "dest": "/api/seller-admin/balance.js"
    },
    {
      "src": "/api/seller-admin/delivery-info",
      "dest": "/api/seller-admin/delivery-info.js"
    },
    {
      "src": "/api/seller-admin/test",
      "dest": "/api/seller-admin/test.js"
    },
    {
      "src": "/api/seller-admin/test-alternative-headers",
      "dest": "/api/seller-admin/test-alternative-headers.js"
    },
    {
      "src": "/api/seller-admin/test-endpoints",
      "dest": "/api/seller-admin/test-endpoints.js"
    },
    {
      "src": "/adwords-tool",
      "dest": "/adwords-tool/tool.html"
    },
    {
      "src": "/adwords-tool/",
      "dest": "/adwords-tool/tool.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
