{
  "name": "sdeal-react-app",
  "version": "1.0.0",
  "description": "SDeal.com React Application",
  "private": true,
  "dependencies": {
    "@mollie/api-client": "^4.0.0",
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^7.7.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^17.2.3",
    "prisma": "^5.7.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup-images": "node scripts/setup-images.js",
    "postinstall": "npm run setup-images",
    "prisma:generate": "prisma generate",
    "vercel-build": "npm install && cd server && npm install && cd .. && npm run prisma:generate && npm run setup-images && npm run build",
    "vercel-build:with-migrations": "npm install && cd server && npm install && cd .. && npm run prisma:generate && npm run prisma:migrate:deploy && npm run setup-images && npm run build",
    "prisma:migrate:deploy": "prisma migrate deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000"
}
